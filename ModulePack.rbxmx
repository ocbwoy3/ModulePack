					elseif peek().Type == 'Ident' then
						-- Field name record
						local ident = get()
						local colon = expect('Symbol', ':')
						local valueType = typeexpr()
						table.insert(records, {
							Type = 'Name';
							Ident = ident;
							ValueType = valueType;
