		local fbody, enTk = blockbody('end')
		--
		return MkNode{
			Type = (isAnonymous and 'FunctionLiteral' or 'FunctionStat');
			NameChain = nameChain;
			ArgList = argList;
			ArgTypeList = argTypeList;
			ReturnType = returnType;
			Body = fbody;
