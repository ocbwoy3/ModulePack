			local genericArgumentList = {}
			local genericArgumentCommaList = {}
			if peek().Source == '<' then
				tkOpenAngle = get()
				-- Parameterized generic type
				while true do
					table.insert(genericArgumentList, typeexpr())
					if peek().Source == ',' then
