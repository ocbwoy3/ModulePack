	function typeexpr()
		local parts = {typeExprPart()}
		local combiners = {}
		local combinersPresent = {}
		while peek().Source == '&' or peek().Source == '|' do
			local combiner = get()
			combinersPresent[combiner.Source] = true
			table.insert(parts, typeExprPart())
			table.insert(combiners, combiner)
